# Replace ___ with your code

def factorial(n):
    num_factorial =1
    for i in range(1,n+1):
        num_factorial *=i
    if n < 0:
       return "Factorial does not exist for negative numbers."
    elif n > 0:
       return num_factorial
    else:
        return 1 
   

# take integer input from the user
user_input = int(input())
# call the function
print(factorial(user_input))
